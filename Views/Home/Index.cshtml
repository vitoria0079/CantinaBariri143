@{
    ViewData["Title"] = "Home Page";
}
<br />
<div class="text-center">
    <h1 class="display-4 h1-home">CANTINA SESI-SP 143</h1>
</div>
<br />
<div id="carouselExampleFade" class="carousel slide carousel-fade">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/img/IMG CANTINA.png"
                 class="d-block img-cantina col mx-auto" width="1000" height="500" alt="primeiro Slide" style="border-radius: 20px;">
        </div>
        <div class="carousel-item">
            <img src="~/img/img cantina 2.png"
                 class="d-block img-cantina col mx-auto" width="1000" height="500" alt="segundo Slide" style="border-radius: 20px;">
        </div>
        <div class="carousel-item">
            <img src="~/img/img cantina 3.png"
                 class="d-block img-cantina col mx-auto" width="1000" height="500" alt="terceiro Slide" style="border-radius: 20px;">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>